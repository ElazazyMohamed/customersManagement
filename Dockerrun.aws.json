{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
    {
      "name": "elazazy-react",
      "image": "elazazymohamed/elazazy-react:latest",
      "essential": true,
      "memory": 256,
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 5173
        }
      ]
    },
    {
      "name": "elazazy-api",
      "image": "elazazymohamed/elazazy-api:02.09.2024.00.58.29",
      "essential": true,
      "memory": 512,
      "portMappings": [
        {
          "hostPort": 8080,
          "containerPort": 8080
        }
      ],
      "environment": [
        {
          "name": "SPRING_PROFILES_ACTIVE",
          "value": "aws"
        }
      ]
    }
  ]
}
